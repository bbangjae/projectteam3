<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <mapper namespace="com.spring_boot.projectTeam.dao.IBookDAO">
  	<resultMap id="prdResult" type="com.spring_boot.projectTeam.model.BookVO">
  	
  	</resultMap>
  		
   <!--  책 상세 조회 page: bNo 전달받아서 bookVO 정보 전달-->
   <select id="detailViewBook" parameterType="string"
   							resultType=	"com.spring_boot.projectTeam.model.BookInfoVO_b">
	
  		select * from book where bookNo=#{bookNo}
  		
  	</select>
  	
  	</mapper>	
  		
  		
  		
  	