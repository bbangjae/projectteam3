<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace= "com.spring_boot.projectTeam.dao.IBookInfoDAO_y">

	<insert id="insertBorrow" parameterType="com.spring_boot.projectTeam.model.BorrowVO">
		INSERT INTO borrow (bookId, giver, giveNo, takeNo)
		VALUES (#{bookId}, #{giver}, 0, 0)
	</insert>

</mapper>